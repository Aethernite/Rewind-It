pipeline {
    agent any
      
    stages {
        stage('Build') {
            steps {
			    dir("server") {
                   sh 'mvn -B -DskipTests clean package'
                }
            }
        }
        stage('Deliver') {
            steps {
			  dir("server/Jenkins/scripts/"){
			    sh "chmod -R 777 ./"
                sh './deliver.sh'
				}
            }
        }
    }
}